<launch>
  <!-- set acados variables according to your system -->
  <env name="LD_LIBRARY_PATH" value="$(optenv HOME)/acados/lib:$(optenv LD_LIBRARY_PATH)" />
  <env name="ACADOS_SOURCE_DIR" value="$(optenv HOME)/acados" />
  

  <node pkg="mpc_controller" type="wbc_controller_node" name="mpc_controller" output="screen">
    <param name="" textfile="$(find mpc_controller)/config/controller_params.yaml" type="yaml"/>
  </node>

  <node pkg="rqt_plot" type="rqt_plot" name="rqt_plot" output="screen">
  </node>

  <node pkg="olympus_observer" type="olympusObserver" name="olympus_observer" output="screen">
    <param name="robot" textfile="$(find olympus_observer)/configs/real_offsets.yaml" type="yaml"/>
  </node>

  <param
  name="robot_description"
  textfile="$(find olympus_description)/urdf/olympus_simple_collision.urdf" />

  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz" 
    args="-d $(find olympus_intergration_helpers)/config/trajectory.rviz" />
  <node
    name="tf_world_Body_link"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 olympus Body 40" />

</launch>